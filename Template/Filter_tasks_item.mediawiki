<includeonly><tr>
<td>[[{{{Page}}}|{{{Task}}}]]</td>
<td>{{{Date_due|}}}</td>
<td>{{#arraymap: {{{Assignees|}}}
      |;
      |@@@
      |{{#queryformlink: form=Filter tasks
         |link text={{Get name|@@@}}
         |Filter tasks[Assignees]=@@@
         |_run=1
       }}
      |\s&middot;\s
    }}</td>
<td>{{#arraymap: {{{Reporters|}}}
      |;
      |@@@
      |{{#queryformlink: form=Filter tasks
         |link text={{Get name|@@@}}
         |Filter tasks[Reporters]=@@@
         |_run=1
       }}
      |\s&middot;\s
    }}</td>
<td>{{#queryformlink: form=Filter tasks
      |link text={{{Status|}}}
      |Filter tasks[Status]={{{Status|}}}
      |_run=1
    }}</td>
<td>{{#arraymap: {{{Labels|}}}
      |;
      |@@@
      |{{#cargo_query: tables=Task_labels
         |fields=Label=Label,Background=Background,Color=Color,_pageName=Page
         |where=_pageTitle='{{#explode:@@@|:|1}}'
         |format=template
         |template=Render label
         |named args=yes
       }}
      |\s&middot;\s
    }}</td>
<td>[[{{{Project|}}}]]</td>
</tr></includeonly>
